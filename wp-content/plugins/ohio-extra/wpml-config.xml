<wpml-config>
    <elementor-widgets>
        <!-- ohio_accordion -->
        <widget name="ohio_accordion">
            <fields-in-item items_of="tabs">
                <field type="Title" editor_type="LINE">list_title</field>
                <field type="Content text" editor_type="VISUAL">list_content_editor</field>
            </fields-in-item>
        </widget>
        <!-- ohio_accordion_horizontal -->
        <widget name="ohio_accordion_horizontal">
            <fields-in-item items_of="tabs">
                <field type="Content text" editor_type="VISUAL">list_content_editor</field>
            </fields-in-item>
        </widget>
        <!-- ohio_banner -->
        <widget name="ohio_banner">
            <fields>
                <field type="Headline" editor_type="LINE">title</field>
                <field type="Subtitle text" editor_type="VISUAL">subtitle</field>
                <field type="Description (HTML allowed)" editor_type="VISUAL">description</field>
                <field type="Link URL" editor_type="LINK" key_of="link">url</field>
            </fields>
        </widget>
        <!-- ohio_button -->
        <widget name="ohio_button">
            <fields>
                <field type="Title" editor_type="LINE">title</field>
                <field type="Link URL" editor_type="LINK" key_of="link">url</field>
            </fields>
        </widget>
        <!-- ohio_carousel -->
        <widget name="ohio_carousel">
            <fields-in-item items_of="tabs">
                <field type="Content text" editor_type="VISUAL">list_content_editor</field>
            </fields-in-item>
        </widget>
        <!-- ohio_circle_progress_bar -->
        <widget name="ohio_circle_progress_bar">
            <fields>
                <field type="Label" editor_type="LINE">label</field>
            </fields>
        </widget>
        <!-- ohio_clients_logo -->
        <widget name="ohio_clients_logo">
            <fields>
                <field type="Details" editor_type="LINE">description</field>
                <field type="Link URL" editor_type="LINK" key_of="link">url</field>
            </fields>
        </widget>
        <!-- ohio_compare -->
        <widget name="ohio_compare">
            <fields>
                <field type="Image before label" editor_type="LINE">before_label</field>
                <field type="Image after label" editor_type="LINE">after_label</field>
            </fields>
        </widget>
        <!-- ohio_counter -->
        <widget name="ohio_counter">
            <fields>
                <field type="Text before" editor_type="LINE">count_text_before</field>
                <field type="Text after" editor_type="LINE">count_text_after</field>
                <field type="Title" editor_type="LINE">title</field>
            </fields>
        </widget>
        <!-- ohio_call_to_action -->
        <widget name="ohio_call_to_action">
            <fields>
                <field type="Heading" editor_type="LINE">title</field>
                <field type="Subtitle" editor_type="AREA">subtitle</field>
                <field type="Link title" editor_type="LINE">button_title</field>
                <field type="Link URL" editor_type="LINK" key_of="button_link">url</field>
            </fields>
        </widget>
        <!-- ohio_dynamic_text -->
        <widget name="ohio_dynamic_text">
            <fields>
                <field type="Text before" editor_type="LINE">before_text</field>
                <field type="Text after" editor_type="LINE">after_text</field>
            </fields>
            <fields-in-item items_of="dynamic_text">
                <field type="Text variant" editor_type="LINE">list_text</field>
            </fields-in-item>
        </widget>
        <!-- ohio_heading -->
        <widget name="ohio_heading">
            <fields>
                <field type="Heading" editor_type="AREA">title</field>
                <field type="Subtitle" editor_type="AREA">subtitle</field>
            </fields>
        </widget>
        <!-- ohio_icon_box -->
        <widget name="ohio_icon_box">
            <fields>
                <field type="Heading" editor_type="LINE">title</field>
                <field type="Description" editor_type="AREA">description</field>
                <field type="Link title" editor_type="LINE">button_title</field>
                <field type="Link URL" editor_type="LINK" key_of="button_link">url</field>
            </fields>
        </widget>
        <!-- ohio_message -->
        <widget name="ohio_message">
            <fields>
                <field type="Message text" editor_type="AREA">text</field>
                <field type="Link title" editor_text="LINE">button_title</field>
                <field type="Link URL" editor_type="LINK" key_of="button_link">url</field>
            </fields>
        </widget>
        <!-- ohio_pricing_list -->
        <widget name="ohio_pricing_list">
            <fields>
                <field type="Headline" editor_type="LINE">name</field>
                <field type="Details" editor_type="LINE">ingredients</field>
                <field type="Regular Price" editor_type="LINE">regular_price</field>
                <field type="Sale Price" editor_type="LINE">sale_price</field>
            </fields>
        </widget>
        <!-- ohio_pricing_table -->
        <widget name="ohio_pricing_table">
            <fields>
                <field type="Headline" editor_type="LINE">headline</field>
                <field type="Subtitle" editor_type="LINE">subtitle</field>
                <field type="Description" editor_type="AREA">description</field>
                <field type="Price" editor_type="LINE">price</field>
                <field type="Price tag" editor_type="AREA">caption</field>
                <field type="Currency" editor_type="LINE">currency</field>
                <field type="Button title" editor_type="LINE">button_title</field>
                <field type="Button URL" editor_type="LINK" key_of="button_link">url</field>
            </fields>
            <fields-in-item items_of="features_list">
                <field type="Healine" editor_type="LINE">list_title</field>
            </fields-in-item>
        </widget>
        <!-- ohio_process -->
        <widget name="ohio_process">
            <fields>
                <field type="Number" editor_type="LINE">number</field>
                <field type="Headline" editor_type="LINE">headline</field>
                <field type="Description" editor_type="LINE">description</field>
            </fields>
        </widget>
        <!-- ohio_progress_bar -->
        <widget name="ohio_progress_bar">
            <fields>
                <field type="Progress bar label" editor_type="AREA">label</field>
            </fields>
        </widget>
        <!-- ohio_service_table -->
        <widget name="ohio_service_table">
            <fields>
                <field type="Headline" editor_type="LINE">headline</field>
                <field type="Subtitle" editor_type="LINE">subtitle</field>
                <field type="Description" editor_type="AREA">description</field>
            </fields>
            <fields-in-item items_of="features_list">
                <field type="Headline" editor_type="LINE">list_title</field>
            </fields-in-item>
        </widget>
        <!-- ohio_social_networks -->
        <widget name="ohio_social_networks">
            <fields-in-item items_of="social_networks">
                <field type="Link URL" editor_type="LINE">list_link</field>
            </fields-in-item>
        </widget>
        <!-- ohio_tabs -->
        <widget name="ohio_tabs">
            <fields-in-item items_of="tabs">
                <field type="Title" editor_type="LINE">list_title</field>
                <field type="Content" editor_type="VISUAL">list_content_editor</field>
            </fields-in-item>
        </widget>
        <!-- ohio_team_member -->
        <widget name="ohio_team_member">
            <fields>
                <field type="Member name" editor_type="LINE">member_name</field>
                <field type="Member position" editor_type="LINE">member_position</field>
                <field type="Member about" editor_type="AREA">member_description</field>
                <field type="Link URL" editor_type="LINK" key_of="team_member_link">url</field>
            </fields>
            <fields-in-item items_of="social_networks">
                <field type="Link URL" editor_type="LINE">list_link</field>
            </fields-in-item>
        </widget>
        <!-- ohio_team_members -->
        <widget name="ohio_team_members">
            <fields-in-item items_of="members">
                <field type="Member name" editor_type="LINE">member_name</field>
                <field type="Member position" editor_type="LINE">member_position</field>
