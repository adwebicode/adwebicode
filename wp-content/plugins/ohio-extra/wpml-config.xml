        <!-- ohio_testimonial -->
        <widget name="ohio_testimonial">
            <fields>
                <field type="Headline" editor_type="LINE">title</field>
                <field type="Testimonial text" editor_type="AREA">testimonial_text</field>
                <field type="Author name" editor_type="LINE">author_name</field>
